{
  "name": "babel-plugin-magic-comments",
  "version": "1.0.4",
  "description": "Adds webpack magic comments to your dynamic imports.",
  "type": "module",
  "main": "dist",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dual": "babel-dual-package src --extensions .ts",
    "build": "npm run build:types && npm run build:dual",
    "lint": "eslint . src __tests__ --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "BABEL_ENV=test jest"
  },
  "keywords": [
    "babel",
    "plugin",
    "webpack",
    "magic comments"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morganney/babel-plugin-magic-comments.git"
  },
  "bugs": {
    "url": "https://github.com/morganney/babel-plugin-magic-comments/issues"
  },
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.22.5",
    "magic-comments": "^2.1.8",
    "schema-utils": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.2",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-dual-package": "^1.0.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.0",
    "jest": "^29.6.2",
    "prettier": "^2.8.8"
  }
}
