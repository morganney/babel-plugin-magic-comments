{
  "name": "babel-plugin-magic-comments",
  "version": "1.0.0-alpha.0",
  "description": "Adds webpack magic comments to your dynamic imports.",
  "type": "module",
  "main": "dist",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "babel src --out-dir dist",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist/cjs --out-file-extension .cjs",
    "lint": "eslint . src __tests__ --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "test": "node --experimental-vm-modules --no-warnings ./node_modules/.bin/jest"
  },
  "keywords": [
    "babel",
    "plugin",
    "webpack",
    "magic",
    "comments"
  ],
  "files": [
    "dist"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "magic-comments": "^1.0.0-rc.0",
    "schema-utils": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8"
  }
}
